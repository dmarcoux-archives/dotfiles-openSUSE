ealias zal='sudo zypper addlock'
ealias zar='sudo zypper addrepo'
ealias zdup='sudo zypper dup --details'
ealias zif='zypper info --provides --requires --recommends --suggests'
ealias zin='sudo zypper install --details'
ealias zint='sudo zypper install --details --type'
ealias zll='zypper locks'
ealias zlr='zypper repos --show-enabled-only --sort-by-priority'
ealias zmr='sudo zypper modifyrepo'
ealias zmrn='sudo zypper modifyrepo --name'
ealias zmrp='sudo zypper modifyrepo --priority'
ealias zre='sudo zypper refresh'
ealias zrl='sudo zypper removelock'
ealias zrm='sudo zypper remove --details'
ealias zse='zypper search --details'
